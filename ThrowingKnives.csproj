<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup Condition="'$(Configuration)'=='Release'">
		<DebugSymbols>False</DebugSymbols>
		<DebugType>None</DebugType>
		<GenerateDependencyFile>false</GenerateDependencyFile>
		<PublishDir>./bin/ThrowingKnives/</PublishDir>
	</PropertyGroup>
	<PropertyGroup>
		<TargetFramework>net8.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
	</PropertyGroup>
	<ItemGroup>
		<Compile Remove="KitsuneMenu\**\*.cs" />
		<EmbeddedResource Remove="KitsuneMenu\**\*" />
		<None Remove="KitsuneMenu\**\*" />
	</ItemGroup>
	<ItemGroup>
		<PackageReference Include="CounterStrikeSharp.API" Version="1.0.352">
			<PrivateAssets>none</PrivateAssets>
			<ExcludeAssets>runtime</ExcludeAssets>
			<IncludeAssets>compile; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
		</PackageReference>
	</ItemGroup>
	<ItemGroup>
		<Reference Include="KitsuneMenu">
			<HintPath>KitsuneMenu\src\bin\Debug\net8.0\KitsuneMenu.dll</HintPath>
			<Private>true</Private>
		</Reference>
	</ItemGroup>
</Project>
